			function submit_search_form()  
			{ 
				var global_qvalue = document.getElementById('qterm_id').value;    		
				document.getElementById("hiddensearchId").action ="http://search.worldbank.org/all"+"?qterm="+global_qvalue;  
				document.getElementById("hiddensearch_id_txt").value= global_qvalue;   
				document.getElementById("hiddensearchId").submit();    
			  	return false; 
			}
		
			function clearField(field)
			{ 
				if(field.value==field.defaultValue)   
					field.value='';
			}
			
			$(function(){
				var url=window.location.href;
				if(url.search("/mn/")!=-1)
				{
					$(".search-text").addClass("search-text-mn");
					$(".search-submit-text").addClass("search-submit-text-mn");
				}
			});
		</script>


	
	
	

	
		
			
		
	
		
	
		
	
		
	
		
	
		
	
		
	
